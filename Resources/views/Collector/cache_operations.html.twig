{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
        <img width="20" height="28" alt="Tbbc Cache"
             src="data:image/png;base64," />
        <span class="sf-toolbar-status">{{ collector.operations | length }}</span>
    {% endset %}

    {% set text %}
        <div class="sf-toolbar-info-piece">
            <b>Cache operations</b>
            <span class="sf-toolbar-status" style="background-color: transparent; color: #666">
                {{ collector.operations | length }}
            </span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Cache hits</b>
            <span class="sf-toolbar-status{% if collector.hits > 0 %} sf-toolbar-status-green{% endif %}">
                {{- collector.hits -}}
            </span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Cache miss</b>
            <span class="sf-toolbar-status{% if collector.miss > 0 %} sf-toolbar-status-yellow{% endif %}">
                {{- collector.miss -}}
            </span>
        </div>
    {% endset %}

    {% include 'WebProfilerBundle:Profiler:toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}

{% block menu %}
    {# the menu content #}
    <span class="label">
        {% spaceless %}
        <span class="icon">
            {#
                Icons License
                =============

                Glyphish icons (http://glyphish.com/) created by Joseph Wain
                (http://www.penandthink.com/) are shared under a Creative Commons Attribution
                license (http://creativecommons.org/licenses/by/3.0/us/).
            #}
            <img src="data:image/png;base64," style="padding-top:5px;" alt="Tbbc Cache" />
        </span>
        {% endspaceless %}
        <strong>Tbbc Cache</strong>
        <span class="count">
            <span>{{ collector.operations | length }}</span>
        </span>
    </span>
{% endblock %}

{% block panel %}
    {{ block('overview') }}
    {{ block('cacheable') }}
    {{ block('cache_evict') }}
    {{ block('cache_update') }}
{% endblock %}

{% block overview %}
    <h2>Overview</h2>

    {% if collector.operations %}
        {%
            set data = {
                'Total cache operations': collector.operations|length,
                'Cacheable operations': collector.cacheableOperations|length,
                'CacheEvict operations': collector.cacheEvictOperations|length,
                'CacheUpdate operations': collector.cacheUpdateOperations|length
            }
        %}

        {% include 'WebProfilerBundle:Profiler:table.html.twig' with {'data': data} only %}
    {% else %}
        <p>
            <em>No cache operations</em>
        </p>
    {% endif %}
{% endblock overview %}

{% block cacheable %}
    <h2>Cacheable Operations</h2>

    {% if collector.cacheableOperations %}
        <table>
            <thead>
            <tr>
                <th scope="col">Class</th>
                <th scope="col">Method</th>
                <th scope="col">Key</th>
                <th scope="col">Hit</th>
                <th scope="col">Miss</th>
                <th scope="col">Caches</th>
            </tr>
            </thead>
            <tbody>
            {% for operation in collector.cacheableOperations %}
                <tr>
                    <td>{{ operation.targetClass }}</td>
                    <td>{{ operation.targetMethod }}</td>
                    <td>{{ operation.key }}</td>
                    <td>{{ (operation.cacheHit is not empty) ? '1 (' ~ operation.cacheHit ~ ')' : '-' }}</td>
                    <td>{{ operation.cacheMiss|length }}</td>
                    <td>
                        <ul>
                            {% for cache in operation.caches %}
                                <li>{{ cache }}</li>
                            {% endfor %}
                        </ul>
                    </td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    {% else %}
        <p>
            <em>No Cacheable operations</em>
        </p>
    {% endif %}
{% endblock cacheable %}

{% block cache_evict %}
    <h2>CacheEvict Operations</h2>

    {% if collector.cacheEvictOperations %}
        <table>
            <thead>
            <tr>
                <th scope="col">Class</th>
                <th scope="col">Method</th>
                <th scope="col">Key</th>
                <th scope="col">All entries ?</th>
                <th scope="col">Before invocation</th>
                <th scope="col">Caches</th>
            </tr>
            </thead>
            <tbody>
            {% for operation in collector.cacheEvictOperations %}
                <tr>
                    <td>{{ operation.targetClass }}</td>
                    <td>{{ operation.targetMethod }}</td>
                    <td>{{ (operation.key is not empty) ? operation.key : ' - '  }}</td>
                    <td>{{ operation.flush ? 'Yes' : 'No'  }}</td>
                    <td>{{ operation.beforeInvocation ? 'Yes' : 'No'  }}</td>
                    <td>
                        <ul>
                            {% for cache in operation.caches %}
                                <li>{{ cache }}</li>
                            {% endfor %}
                        </ul>
                    </td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    {% else %}
        <p>
            <em>No CacheEvict operations</em>
        </p>
    {% endif %}
{% endblock cache_evict %}

{% block cache_update %}
    <h2>CacheUpdate Operations</h2>

    {% if collector.cacheUpdateOperations %}
        <table>
            <thead>
            <tr>
                <th scope="col">Class</th>
                <th scope="col">Method</th>
                <th scope="col">Key</th>
                <th scope="col">Caches</th>
            </tr>
            </thead>
            <tbody>
            {% for operation in collector.cacheUpdateOperations %}
                <tr>
                    <td>{{ operation.targetClass }}</td>
                    <td>{{ operation.targetMethod }}</td>
                    <td>{{ (operation.key is not empty) ? operation.key : ' - '  }}</td>
                    <td>
                        <ul>
                            {% for cache in operation.cacheUpdates %}
                                <li>{{ cache }}</li>
                            {% endfor %}
                        </ul>
                    </td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    {% else %}
        <p>
            <em>No CacheUpdate operations</em>
        </p>
    {% endif %}
{% endblock cache_update %}